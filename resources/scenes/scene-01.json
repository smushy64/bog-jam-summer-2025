        },

        {
            "type": "story",
            "id": 9079,
            "story.text": "Stay away from my sister! Y'hear?\n<rgba:7f,7f,7f,ff>Best take care of Zuma, y'hear?",
            "story.character": "Stefan",
            "story.animation.name": "stefan",
            "story.animation.side": "left"
        },
        {
            "type": "story",
            "id": 9080,
            "story.text": "W-what ? ? ?",
            "story.character": "Jade",
            "story.animation.name": "jade_surprise",
            "story.animation.side": "right"
        },
        {
            "type": "story",
            "id": 9081,
            "story.text": "You know what I mean, missy.",
            "story.character": "Stefan",
            "story.animation.name": "stefan",
            "story.animation.side": "left"
        },
        {
            "type": "story",
            "id": 9080,
            "story.text": ". . .",
            "story.character": "Jade",
            "story.animation.name": "jade_neutral",
            "story.animation.side": "right"
        },
